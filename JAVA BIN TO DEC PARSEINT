import java.util.*;
public class Main
{
    public static void main(String[] args)
    {
        Scanner s=new Scanner(System.in);
        String binary;
        System.out.println("Enter a binary value");
        binary =s.nextLine();
        System.out.println("Input value in base 10 = "+Integer.parseInt(binary,2));
        System.out.println("Input value in base 2 = "+binary);
        
    }
}
